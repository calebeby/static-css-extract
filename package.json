    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@types/es-module-lexer": "0.3.0",
    "@types/jest": "26.0.14",
    "acorn": "8.0.4",
    "babel-jest": "26.5.2",
    "babel-plugin-const-enum": "^1.0.1",
    "esm": "^3.2.25",
    "jest": "^26.5.3",
    "jest-circus": "^26.5.3",
    "magic-string": "0.25.7",
    "nodemon": "^2.0.5",
    "prettier": "2.1.2",
    "rollup": "2.32.0",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
